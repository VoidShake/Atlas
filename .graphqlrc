documents: graphql/queries/**/*.gql
schema:
   - http://localhost:8080/api/graphql
   - graphql/localSchema.gql

extensions:
   codegen:
      generates:
         ./graphql/generated.ts:
            plugins:
               - typescript
               - typescript-operations
               - typed-document-node
               - fragment-matcher
               - add:
                    content: /* eslint-disable */
      config:
         dedupeFragments: true
         scalars:
            DateTime: number
